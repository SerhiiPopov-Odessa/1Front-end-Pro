{
  "name": "booking",
  "private": true,
  "version": "0.0.0",
  "description": "Deploy JSON Server to Vercel",
  "main": "api/server.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "json-server --watch db.json",
    "start": "node api/server.js",
    "local:start": "concurrently -k \"npm run dev\" \"npm run server\"",
    "test:cy": "cypress open",
    "test:e2e": "concurrently -k \"npm run dev\" \"npm run server\" \"wait-on http://localhost:5173 http://localhost:3000 && npm run test:cy\"",
    "test:e2e-video": "concurrently -k \"npm run dev\" \"npm run server\" \"wait-on http://localhost:5173 http://localhost:3000 && npx cypress run\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/lab": "^6.0.0-beta.23",
    "@mui/material": "^6.5.0",
    "@mui/x-date-pickers": "^7.24.0",
    "@reduxjs/toolkit": "^2.5.0",
    "@tanstack/react-query": "^5.90.20",
    "axios": "^1.13.2",
    "dayjs": "^1.11.13",
    "history": "^5.3.0",
    "json-server": "^0.17.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-final-form": "^6.5.9",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.25.1",
    "redux": "^5.0.1",
    "redux-first-history": "^5.2.0",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "cypress": "^15.9.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "vite": "^6.0.1",
    "wait-on": "^9.0.3"
  }
}
